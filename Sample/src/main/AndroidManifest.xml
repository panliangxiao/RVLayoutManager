<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.app.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:name=".AppApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CardSwipeActivity" />

        <activity android:name=".PickerActivity" />

        <activity android:name=".GalleryActivity" />

        <activity android:name=".AdapterActivity" />

        <activity android:name=".nested.NestedActivity" />

        <activity android:name=".suspension.SuspensionActivity"/>
    </application>

</manifest>